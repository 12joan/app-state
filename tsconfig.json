{
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["node_modules"],

  "compilerOptions": {
    "target": "es2016",
    "module": "es2015",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
